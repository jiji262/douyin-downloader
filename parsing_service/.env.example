# Flask配置
FLASK_ENV=production
FLASK_DEBUG=false
PORT=5000

# Redis配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_DB=0

# 策略开关
ENABLE_PLAYWRIGHT=true
ENABLE_SELENIUM=false

# 代理配置
USE_PROXY=false
PROXY_LIST=

# 日志级别
LOG_LEVEL=INFO

# 速率限制
RATE_LIMIT_DEFAULT=200 per day
RATE_LIMIT_PARSE=10 per minute
RATE_LIMIT_BATCH=5 per minute

# 缓存配置
CACHE_TTL=3600
CACHE_MAX_SIZE=10000

# 性能配置
MAX_WORKERS=10
REQUEST_TIMEOUT=30
BROWSER_TIMEOUT=30